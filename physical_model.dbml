TABLE City {
  city_id SERIAL [PRIMARY KEY]
  city_name VARCAHR(100)
}

TABLE Address {
  address_id SERIAL [primary key]
  city_id integer [not null]
  address VARCHAR(255) [not null]
  postal_code CHAR(5) [not null]
}
REF: Address.city_id > City.city_id


TABLE Organizer {
  organizer_id SERIAL [primary key]
  organizer_name VARCHAR(100) [not null]
  phone VRCAHR(20) UNIQUE [not null]
  email VARCHAR(255) UNIQUE [not null]
  address_id integer [not null]
}
REF: Organizer.organizer_id - Address.address_id


TABLE School {
  school_id SERIAL [PRIMARY KEY]
  organizer_id integer [not null]
  address_id integer [not null]
  schoolname VARCHAR(100)
  phone VARCHAR(20) UNIQUE [not null]
  eamil VARCHAR(255) UNIQUE [not null]
  open_date DATE [not null]
}
REF: School.organizer_id > Organizer.organizer_id


TABLE Program {
  program_id SERIAL [not null]
  program_name VARCHAR(100) [not null]
  nr_active INTEGER
}

TABLE SchoolProgramClass {
  school_id integer [not null]
  program_id integer [not null]
  class_id integer [not null]
  indexes {
    (school_id,program_id,class_id) [PK]
  }
}
REF: SchoolProgramClass.school_id > School.school_id
REF: SchoolProgramClass.program_id > Program.program_id
REF: SchoolProgramClass.class_id > Class.class_id


TABLE Class {
  class_id SERIAL [primary key]
  class_name varchar(50)
}

TABLE Student {
  student_id SERIAL [primary key]
  first_name varchar(50) [not null]
  last_name varchar(50) [not null]
  class_id integer 
}
REF: Student.class_id > Class.class_id

Table Student_info {
  social_security_nr VARCHAR(15) [primary key]
  student_id integer [NOT NULL]
  address_id integer [not null]
  phone VARCHAR(20) UNIQUE [not null]
  email VARCHAR(255) UNIQUE [not null]
}
REF: Student_info.student_id - Student.student_id
REF: Student_info.address_id - Address.address

Table Staff {
  staff_id SERIAL [primary key]
  school_id integer [not null]
  first_name varchar(50) [not null]
  last_name varchar(50) [not null]
  phone varchar(20) UNIQUE [not null]
  email varchar(255) UNIQUE [not null]
  work_title varchar(50) [not null]
  roll varchar(25) [not null] 
}
REF: Staff.school_id > School.school_id


Table Consultant {
  consultant_id SERIAL [primary key]
  fee_per_hour integer [not null]
  organization_nr varchar(11) [not null]
  staff_id integer [not null]
}
REF: Consultant.organization_nr > Consultant_company.organization_nr
REF: Consultant.staff_id - Staff.staff_id


TABLE Consultant_company {
  organization_nr VARCHAR(11) [primary key]
  company_name varchar(100) [not null]
  f_tax bool [not null]
  phone varchar(20) UNIQUE [not null]
  email varchar(255) UNIQUE [not null]
}

TABLE Employee_info {
  social_security_nr varchar(15) [primary key]
  staff_id integer [not null]
  address_id integer [not null]
  salery_per_month integer [not null]
}
REF: Employee_info.staff_id - Staff.staff_id
REF: Employee_info.address_id - Address.address_id

Table Teacher {
  teacher_id SERIAL [primary key]
  staff_id integer [not null]
}
REF: Teacher.staff_id - Staff.staff_id

Table Standalonecourse {
  standalonecourse_id SERIAL [primary key]
  course_code varchar(25) [not null]
  starts_date date [not null]
  ends_date date [not null]
}
REF: Standalonecourse.course_code - Course_info.course_code

Table Enrollment {
  standalonecourse_id integer [not null]
  student_id integer [not null]
  school_id integer [not null]
  indexes {
    (standalonecourse_id,student_id,school_id) [pk]
  }
}
REF: Enrollment.standalonecourse_id > Standalonecourse.standalonecourse_id
REF: Enrollment.student_id > Student.student_id
REF: Enrollment.school_id - School.school_id

Table Course_info {
  course_code varchar(25) [primary key]
  course_name varchar(50) [not null]
  credits integer [not null]
  descriptions text [not null]
}


Table Course {
  course_id SERIAL [primary key]
  course_code varchar(50) [not null]
}
REF: Course.course_code - Course_info.course_code


Table TeacherAssignment {
  teacher_id integer [not null]
  standalonecourse_id integer [not null]
  school_id integer [not null]
  indexes {
    (teacher_id,standalonecourse_id,school_id) [pk]
  }
}
REF: TeacherAssignment.teacher_id > Teacher.teacher_id
REF: TeacherAssignment.standalonecourse_id > Standalonecourse.standalonecourse_id
REF: TeacherAssignment.school_id > School.school_id


Table EducationalManagement {
  management_id SERIAL [primary key]
  class_id integer [not null]
  staff_id integer [not null]
}
REF: EducationalManagement.class_id < Class.class_id
REF: EducationalManagement.staff_id - Staff.staff_id

Table TeacherCourse {
  techer_id integer [not null]
  course_id integer [not null]
  indexes {
    (techer_id,course_id) [pk]
  }
}
REF: TeacherCourse.techer_id > Teacher.teacher_id
REF: TeacherCourse.course_id > Course.course_id


Table CourseProgram {
  course_id integer [not null]
  program_id integer [not null]
  starts_date date
  ends_date date
  indexes {
    (course_id,program_id) [pk]
  }
}
REF: CourseProgram.course_id > Course.course_id
REF: CourseProgram.program_id > Program.program_id

